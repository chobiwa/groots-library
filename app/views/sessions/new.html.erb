<div style="text-align:center;">
<h2>Librarian Login page</h2>
<%= form_with(scope: :session ,url: login_path,local:true) do |librarian| %>
  <p>
    <%= librarian.label :email %><br/>
    <%= librarian.text_field :email%>
  </p>
  <p>
    <%= librarian.label :password %><br/>
    <%= librarian.text_field :password %>
  </p>
  <p><%= librarian.submit 'login' %></p>
    <p><%= link_to 'home' ,root_path %></p>
<% end %>
</div>